  },
  "dependencies": {
    "@types/mocha": "^7.0.2",
    "agent": "^0.2.1",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
